{
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "enum": ["api.media.read"]
    },
    "ver": {
      "type": "string"
    },
    "ts": {
      "type": "string"
    },
    "params": {
      "type": "object",
      "properties": {
        "msgid": {
          "type": "string"
        }
      },
      "required": ["msgid"],
      "additionalProperties": false
    },
    "request": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "properties": {
          "fileName": {
            "type": "string",
            "description": "Name of the file"
          },
          "mimeType": {
            "type": "string",
            "description": "mime type  of the file"
          },
          "mediaType": {
            "type": "string",
            "description": "media type of the file"
          },
          "src": {
            "type": "string",
            "enum": ["media/content", "media/questionSet", "media/question"],
            "description": "which category media belong to "
          }
        },
        "required": ["fileName", "src", "mimeType", "mediaType"],
        "additionalProperties": false
      }
    }
  },
  "required": ["id", "ver", "ts", "params", "request"],
  "additionalProperties": false
}
